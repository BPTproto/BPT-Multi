	},
	"require": {
		"php": ">=8.0",
		"ext-mbstring": "*",
		"ext-curl": "*"
	},
	"suggest": {
		"ext-zip": "Needed for using zip method in tools class",
		"ext-openssl": "Needed for using codec method in tools class",
		"ext-mysqli": "Needed for using mysql database"
	},
	"autoload": {
		"psr-4": {
			"BPT\\": "src/"
		},
		"files": [
			"src/functions.php"
		]
	},
	"config": {
		"classmap-authoritative": true
	}
