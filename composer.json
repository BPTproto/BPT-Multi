{
  "name": "bpt/bpt-multi",
  "description": "A simple library for manage and handle telegram bots(multi file version)",
  "keywords": [
    "bpt",
    "bptproto",
    "bptlib",
    "bpt library",
    "telegram",
    "telegram api",
    "bot",
    "bots",
    "robot",
    "api"
  ],
