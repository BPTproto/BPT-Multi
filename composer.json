  "support": {
    "email": "support@bptlib.ir",
    "issues": "https://github.com/BPTproto/BPT-Multi/issues",
    "source": "https://github.com/BPTproto/BPT-Multi"
  },
  "require": {
    "php": ">=8.0",
    "ext-mbstring": "*"
  },
  "suggest": {
    "ext-zip": "Needed for using zip method in tools class",
    "ext-openssl": "Needed for using crypto method in tools class"
  },
  "autoload": {
    "psr-4": {
      "BPT\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "config": {
    "classmap-authoritative": true
  }
}